<?xml version="1.0" standalone='no'?><!--*-nxml-*-->
<!DOCTYPE service-group SYSTEM "avahi-service.dtd">
<service-group>
  <name replace-wildcards="yes">RT Box 3 on %h</name>
  <service>
    <type>_rtbox._tcp</type>
    <port>9999</port>
    <txt-record>RT=Box=3</txt-record>
  </service>
  <service>
    <type>_http._tcp</type>
    <port>80</port>
  </service>
</service-group>


